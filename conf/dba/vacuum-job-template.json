{
    "apiVersion": "extensions/v1beta1",
    "kind": "Job",
    "metadata": {
        "name": "{{.VAC_HOST}}-vacuum-job"
    },
    "spec": {
        "selector": {
            "matchLabels": {
                "app": "{{.VAC_HOST}}-vacuum-job"
            }
        },
        "template": {
            "metadata": {
                "name": "{{.VAC_HOST}}-vacuum-job",
                "labels": {
                    "app": "{{.VAC_HOST}}-vacuum-job"
                }
            },
            "spec": {
                "volumes": [ ],
                "containers": [{
                    "name": "vacuum-job",
                    "image": "crunchydata/crunchy-vacuum",
                    "volumeMounts": [ ],
                    "env": [{
                        "name": "VAC_FULL",
                        "value": "{{.VAC_FULL}}"
                    }, {
                        "name": "VAC_HOST",
                        "value": "{{.VAC_HOST}}"
                    }, {
                        "name": "VAC_ANALYZE",
                        "value": "{{.VAC_ANALYZE}}"
                    }, {
                        "name": "VAC_ALL",
                        "value": "{{.VAC_ALL}}"
                    }, {
                        "name": "VAC_VERBOSE",
                        "value": "{{.VAC_VERBOSE}}"
                    }, {
                        "name": "VAC_FREEZE",
                        "value": "{{.VAC_FREEZE}}"
                    }, {
                        "name": "VAC_TABLE",
                        "value": "{{.VAC_TABLE}}"
                    }, {
                        "name": "PG_USER",
                        "value": "{{.PG_USER}}"
                    }, {
                        "name": "PG_PORT",
                        "value": "{{.PG_PORT}}"
                    }, {
                        "name": "PG_DATABASE",
                        "value": "{{.PG_DATABASE}}"
                    }, {
                        "name": "PG_PASSWORD",
                        "value": "{{.PG_PASSWORD}}"
                    }]
                }],
                "restartPolicy": "Never"
            }
        }
    }
}
